colorTable, texPos2);\n\n\t\tlowp vec4 newColor = mix(newColor1, newColor2, fract(blueColor));\n\t\tgl_FragColor = vec4(mix(textureColor.rgb, newColor.rgb, intensity), alpha);\n\t}\n"},function(t,e,i){"use strict";(function(t){function e(t){return t&&t.__esModule?t:{default:t}}var n=e(i(5)),r=e(i(39));!function(){function e(){this._overlay=null,this._value=1,this._cropWidth=100,this._cropHeight=100,this.programs={},this.programs.overlay="applyOverlayBlend",this.programs.screen="applyScreenBlend",this.programs.multiply="applyMultiplyBlend",this.programs.screen_async="onScreenLoop",this.programs.multiply_async="onMultiplyLoop",this.programs.overlay_async="onFilterLoop"}var o=i(40),s=o.loop,a=(i(49).round,i(31).lines,i(6).__renderableContext),l=i(6).__photoFrameContext;e.prototype={constructor:e,applyFilter:function(t,e,i,n,r,o){return!!this.changed&&(this[this.programs[this.blendType]](t,e),!0)},applyScreenBlend:function(t,e){for(var i,n,r,o,s,a,l,h,u,c,p,d,f,_,g=t.data,A=e.data,y=this.overlay.data,m=Math.floor(g.length),v=t.width,x=t.height,T=this.overlay.width,E=this.overlay.height,b=this.value,C=.99<b,w=0;w<m;w+=4)n=4*(T*(((i=w/4|0)/v|0)/x*E|0)+(i%v/v*T|0)),r=g[w]/255,o=g[w+1]/255,s=g[w+2]/255,a=g[w+3]/255,l=y[n]/255,h=y[1+n]/255,u=y[2+n]/255,p=r+(c=y[3+n]/255)*((p=1-(1-l)*(1-r))-r),d=o+c*((d=1-(1-h)*(1-o))-o),f=s+c*((f=1-(1-u)*(1-s))-s),_=a+c*((_=1-(1-c)*(1-a))-a),C?(A[w]=255*p|0,A[w+1]=255*d|0,A[w+2]=255*f|0,A[w+3]=255*_|0):(A[w]=255*(r+b*(p-r))|0,A[w+1]=255*(o+b*(d-o))|0,A[w+2]=255*(s+b*(f-s))|0,A[w+3]=255*(a+b*(_-a))|0)},applyMultiplyBlend:function(t,e){for(var i,n,r,o,s,a,l,h,u,c,p,d,f,_,g=t.data,A=e.data,y=this.overlay.data,m=Math.floor(g.length),v=t.width,x=t.height,T=this.overlay.width,E=this.overlay.height,b=this.value,C=.99<b,w=0;w<m;w+=4)n=4*(T*(((i=w/4|0)/v|0)/x*E+.5|0)+(i%v/v*T+.5|0)),r=g[w]/255,o=g[w+1]/255,s=g[w+2]/255,a=g[w+3]/255,l=y[n]/255,h=y[1+n]/255,u=y[2+n]/255,p=l*r+l*(1-a)+r*(1-(c=y[3+n]/255)),d=h*o+h*(1-a)+o*(1-c),f=u*s+u*(1-a)+s*(1-c),_=c*